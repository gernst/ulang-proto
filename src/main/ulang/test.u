definition
  f True  x y = x;
  f False x y = y;
  eq x x = True;
  eq x y = False;
  f a b c = (a,b,c);
  g = \a b c d e f -> \z -> (a,b,c,d,e,f, z);
  h a a = (a, B);
  f x A = x;
  f y B = y;
  split (f z) (x,y) = (f, z, x, y);
  numbers n = [n; numbers (Succ n)];
  head [x; xs] = x;
  0 = Zero;
  1 = Succ 0;
  2 = Succ 1;
  3 = Succ 2;
end

eval
  f True  A B;
  f False A B;
  
  let x = A in x;
  
  eq A A;
  
  f A B C;
  g A B C D E F Z;
  h A A;
  
  split (A B) (C, D);
  
  match (A,B) with
    (x,C) -> x
  | z -> (z, C);
  
  numbers 0;
  head (numbers 0);
end
